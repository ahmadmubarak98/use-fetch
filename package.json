    "husky": "^9.0.11",
    "jsdom": "^24.0.0",
    "lint-staged": "^15.2.2",
    "prettier": "^3.2.5",
    "react": "^18.2.0",
    "storybook": "^8.0.8",
    "typescript": "^5.4.5",
    "vite": "^5.2.9",
